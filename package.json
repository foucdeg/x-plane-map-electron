{
  "name": "x-plane-map-electron",
  "productName": "X-Plane-Map",
  "description": "Electron revamp of X-Plane-Map",
  "version": "2.0.2",
  "author": "Foucauld Degeorges <foucauld.degeorges@gmail.com>",
  "homepage": "https://github.com/foucdeg/x-plane-map-electron/",
  "license": "Beerware",
  "repository": {
    "type": "git",
    "url": "git://github.com/foucdeg/x-plane-map-electron.git"
  },
  "main": "app/background.js",
  "build": {
    "appId": "net.fouc.x-plane-map",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "publish": {
        "provider": "github"
      }
    }
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "build": "./node_modules/.bin/webpack",
    "precompile": "npm run build",
    "compile": "electron-builder",
    "start": "./node_modules/.bin/electron .",
    "watch": "./node_modules/.bin/webpack --watch",
    "fake": "node ./src/faker.js"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "electron-context-menu": "^0.8.0",
    "electron-default-menu": "^1.0.1",
    "electron-settings": "^2.2.4",
    "express": "^4.15.4",
    "fs-jetpack": "^0.10.2",
    "geolib": "^2.0.23",
    "google-maps": "^3.2.1",
    "ip": "^1.1.5",
    "jquery": "^3.2.1",
    "leaflet": "^1.2.0",
    "leaflet-hotline": "^0.4.0",
    "leaflet-rotatedmarker": "^0.2.0",
    "leaflet.gridlayer.googlemutant": "^0.6.2",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "qrcode.react": "^0.7.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-leaflet": "^1.5.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-electron": "^1.4.15",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "electron": "^1.7.6",
    "electron-builder": "^19.19.0",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "minimist": "^1.2.0",
    "source-map-support": "^0.4.15",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.2"
  }
